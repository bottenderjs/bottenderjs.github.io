{
  "name": "bottender-docs",
  "description": "The Bottender docs",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && gh-pages -d public -b master -m 'Auto-generated commit [ci skip]'",
    "dev": "gatsby develop",
    "dev:mobile": "gatsby develop -H 0.0.0.0",
    "develop": "npm run dev",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint 'src/**/*.js'",
    "lint:staged": "lint-staged",
    "precommit": "npm run lint:css && lint-staged",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "gatsby": "^1.9.82",
    "gatsby-link": "^1.6.17",
    "gatsby-plugin-glamor": "^1.6.8",
    "gatsby-plugin-google-analytics": "^1.0.10",
    "gatsby-plugin-offline": "^1.0.10",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-react-next": "^1.0.4",
    "gatsby-plugin-sitemap": "^1.2.5",
    "gatsby-plugin-styled-components": "^1.0.5",
    "gatsby-plugin-typography": "^1.7.10",
    "gatsby-remark-autolink-headers": "^1.4.7",
    "gatsby-remark-images": "^1.5.24",
    "gatsby-remark-prismjs": "^1.2.8",
    "gatsby-source-filesystem": "^1.5.2",
    "gatsby-transformer-remark": "^1.7.15",
    "glamorous": "^4.9.7",
    "lodash": "^4.17.4",
    "react": "^16.0.0",
    "react-burger-menu": "^2.1.9",
    "react-helmet": "^5.2.0",
    "styled-components": "^2.2.1",
    "styled-normalize": "^2.2.1",
    "typography": "^0.16.6",
    "typography-theme-kirkham": "^0.16.3"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-yoctol": "^0.14.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "stylelint": "^8.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-yoctol": "^0.7.0",
    "stylelint-processor-styled-components": "^1.0.0"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
